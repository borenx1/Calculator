<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InputFragment">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_variable"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/button_7"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/button_constant"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_variable"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_constant"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_variable"
        app:layout_constraintBottom_toBottomOf="@id/button_variable"
        app:layout_constraintLeft_toRightOf="@+id/button_variable"
        app:layout_constraintRight_toLeftOf="@+id/button_left_bracket"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_constant"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_left_bracket"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_variable"
        app:layout_constraintBottom_toBottomOf="@id/button_variable"
        app:layout_constraintLeft_toRightOf="@+id/button_constant"
        app:layout_constraintRight_toLeftOf="@+id/button_right_bracket"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_left_bracket"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_right_bracket"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_variable"
        app:layout_constraintBottom_toBottomOf="@id/button_variable"
        app:layout_constraintLeft_toRightOf="@id/button_left_bracket"
        app:layout_constraintRight_toRightOf="parent"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_right_bracket"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toBottomOf="@id/button_variable"
        app:layout_constraintBottom_toTopOf="@id/button_4"
        app:layout_constraintLeft_toLeftOf="@id/button_variable"
        app:layout_constraintRight_toRightOf="@id/button_variable"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_seven"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_7"
        app:layout_constraintBottom_toBottomOf="@id/button_7"
        app:layout_constraintLeft_toLeftOf="@id/button_constant"
        app:layout_constraintRight_toRightOf="@id/button_constant"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_eight"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_7"
        app:layout_constraintBottom_toBottomOf="@id/button_7"
        app:layout_constraintLeft_toLeftOf="@id/button_left_bracket"
        app:layout_constraintRight_toRightOf="@id/button_left_bracket"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_nine"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_power"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_7"
        app:layout_constraintBottom_toBottomOf="@id/button_7"
        app:layout_constraintLeft_toLeftOf="@id/button_right_bracket"
        app:layout_constraintRight_toRightOf="@id/button_right_bracket"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_power"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toBottomOf="@id/button_7"
        app:layout_constraintBottom_toTopOf="@id/button_1"
        app:layout_constraintLeft_toLeftOf="@id/button_variable"
        app:layout_constraintRight_toRightOf="@id/button_variable"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_four"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_4"
        app:layout_constraintBottom_toBottomOf="@id/button_4"
        app:layout_constraintLeft_toLeftOf="@id/button_constant"
        app:layout_constraintRight_toRightOf="@id/button_constant"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_five"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_4"
        app:layout_constraintBottom_toBottomOf="@id/button_4"
        app:layout_constraintLeft_toLeftOf="@id/button_left_bracket"
        app:layout_constraintRight_toRightOf="@id/button_left_bracket"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_six"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_divide"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_4"
        app:layout_constraintBottom_toBottomOf="@id/button_4"
        app:layout_constraintLeft_toLeftOf="@id/button_right_bracket"
        app:layout_constraintRight_toRightOf="@id/button_right_bracket"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_divide"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toBottomOf="@id/button_4"
        app:layout_constraintBottom_toTopOf="@id/button_0"
        app:layout_constraintLeft_toLeftOf="@id/button_variable"
        app:layout_constraintRight_toRightOf="@id/button_variable"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_one"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_1"
        app:layout_constraintBottom_toBottomOf="@id/button_1"
        app:layout_constraintLeft_toLeftOf="@id/button_constant"
        app:layout_constraintRight_toRightOf="@id/button_constant"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_two"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_1"
        app:layout_constraintBottom_toBottomOf="@id/button_1"
        app:layout_constraintLeft_toLeftOf="@id/button_left_bracket"
        app:layout_constraintRight_toRightOf="@id/button_left_bracket"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_three"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_times"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_1"
        app:layout_constraintBottom_toBottomOf="@id/button_1"
        app:layout_constraintLeft_toLeftOf="@id/button_right_bracket"
        app:layout_constraintRight_toRightOf="@id/button_right_bracket"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_times"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_0"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toBottomOf="@id/button_1"
        app:layout_constraintBottom_toTopOf="@id/button_delete"
        app:layout_constraintLeft_toLeftOf="@id/button_variable"
        app:layout_constraintRight_toRightOf="@id/button_variable"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_zero"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_point"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_0"
        app:layout_constraintBottom_toBottomOf="@id/button_0"
        app:layout_constraintLeft_toLeftOf="@id/button_constant"
        app:layout_constraintRight_toRightOf="@id/button_constant"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_point"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_plus"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_0"
        app:layout_constraintBottom_toBottomOf="@id/button_0"
        app:layout_constraintLeft_toLeftOf="@id/button_left_bracket"
        app:layout_constraintRight_toRightOf="@id/button_left_bracket"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_plus"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_minus"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_0"
        app:layout_constraintBottom_toBottomOf="@id/button_0"
        app:layout_constraintLeft_toLeftOf="@id/button_right_bracket"
        app:layout_constraintRight_toRightOf="@id/button_right_bracket"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_minus"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_delete"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        app:layout_constraintTop_toBottomOf="@id/button_0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/button_clear"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_delete"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_clear"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_delete"
        app:layout_constraintBottom_toBottomOf="@id/button_delete"
        app:layout_constraintLeft_toRightOf="@id/button_delete"
        app:layout_constraintRight_toLeftOf="@id/button_execute"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_clear"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_execute"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/button_delete"
        app:layout_constraintBottom_toBottomOf="@id/button_delete"
        app:layout_constraintLeft_toRightOf="@id/button_clear"
        app:layout_constraintRight_toRightOf="parent"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_execute"/>

</androidx.constraintlayout.widget.ConstraintLayout>