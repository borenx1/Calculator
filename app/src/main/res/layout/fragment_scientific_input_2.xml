<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InputFragment">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_variable"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/button_e"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/button_constant"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_variable"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_constant"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_variable"
        app:layout_constraintBottom_toBottomOf="@id/button_variable"
        app:layout_constraintLeft_toRightOf="@id/button_variable"
        app:layout_constraintRight_toLeftOf="@id/button_pi"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_constant"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_pi"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_variable"
        app:layout_constraintBottom_toBottomOf="@id/button_variable"
        app:layout_constraintLeft_toRightOf="@id/button_constant"
        app:layout_constraintRight_toLeftOf="@id/button_asin"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_pi"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_asin"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_variable"
        app:layout_constraintBottom_toBottomOf="@id/button_variable"
        app:layout_constraintLeft_toRightOf="@id/button_pi"
        app:layout_constraintRight_toLeftOf="@id/button_acos"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_asin"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_acos"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_variable"
        app:layout_constraintBottom_toBottomOf="@id/button_variable"
        app:layout_constraintLeft_toRightOf="@id/button_asin"
        app:layout_constraintRight_toLeftOf="@id/button_atan"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_acos"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_atan"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_variable"
        app:layout_constraintBottom_toBottomOf="@id/button_variable"
        app:layout_constraintLeft_toRightOf="@id/button_acos"
        app:layout_constraintRight_toRightOf="parent"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_atan"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_e"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toBottomOf="@id/button_variable"
        app:layout_constraintBottom_toTopOf="@id/button_csc"
        app:layout_constraintLeft_toLeftOf="@id/button_variable"
        app:layout_constraintRight_toRightOf="@id/button_variable"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_e"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_i"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_e"
        app:layout_constraintBottom_toBottomOf="@id/button_e"
        app:layout_constraintLeft_toLeftOf="@id/button_constant"
        app:layout_constraintRight_toRightOf="@id/button_constant"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_i"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_tau"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_e"
        app:layout_constraintBottom_toBottomOf="@id/button_e"
        app:layout_constraintLeft_toLeftOf="@id/button_pi"
        app:layout_constraintRight_toRightOf="@id/button_pi"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_tau"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_sinh"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_e"
        app:layout_constraintBottom_toBottomOf="@id/button_e"
        app:layout_constraintLeft_toLeftOf="@id/button_asin"
        app:layout_constraintRight_toRightOf="@id/button_asin"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_sinh"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_cosh"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_e"
        app:layout_constraintBottom_toBottomOf="@id/button_e"
        app:layout_constraintLeft_toLeftOf="@id/button_acos"
        app:layout_constraintRight_toRightOf="@id/button_acos"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_cosh"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_tanh"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_e"
        app:layout_constraintBottom_toBottomOf="@id/button_e"
        app:layout_constraintLeft_toLeftOf="@id/button_atan"
        app:layout_constraintRight_toRightOf="@id/button_atan"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_tanh"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_csc"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toBottomOf="@id/button_e"
        app:layout_constraintBottom_toTopOf="@id/button_abs"
        app:layout_constraintLeft_toLeftOf="@id/button_variable"
        app:layout_constraintRight_toRightOf="@id/button_variable"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_csc"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_sec"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_csc"
        app:layout_constraintBottom_toBottomOf="@id/button_csc"
        app:layout_constraintLeft_toLeftOf="@id/button_constant"
        app:layout_constraintRight_toRightOf="@id/button_constant"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_sec"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_cot"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_csc"
        app:layout_constraintBottom_toBottomOf="@id/button_csc"
        app:layout_constraintLeft_toLeftOf="@id/button_pi"
        app:layout_constraintRight_toRightOf="@id/button_pi"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_cot"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_asinh"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_csc"
        app:layout_constraintBottom_toBottomOf="@id/button_csc"
        app:layout_constraintLeft_toLeftOf="@id/button_asin"
        app:layout_constraintRight_toRightOf="@id/button_asin"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_asinh"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_acosh"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_csc"
        app:layout_constraintBottom_toBottomOf="@id/button_csc"
        app:layout_constraintLeft_toLeftOf="@id/button_acos"
        app:layout_constraintRight_toRightOf="@id/button_acos"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_acosh"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_atanh"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_csc"
        app:layout_constraintBottom_toBottomOf="@id/button_csc"
        app:layout_constraintLeft_toLeftOf="@id/button_atan"
        app:layout_constraintRight_toRightOf="@id/button_atan"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_atanh"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_abs"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toBottomOf="@id/button_csc"
        app:layout_constraintBottom_toTopOf="@id/button_npr"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/button_arg"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_abs"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_arg"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_abs"
        app:layout_constraintBottom_toBottomOf="@id/button_abs"
        app:layout_constraintLeft_toRightOf="@id/button_abs"
        app:layout_constraintRight_toLeftOf="@id/button_conj"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_arg"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_conj"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_abs"
        app:layout_constraintBottom_toBottomOf="@id/button_abs"
        app:layout_constraintLeft_toRightOf="@id/button_arg"
        app:layout_constraintRight_toLeftOf="@id/button_cubed"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_conj"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_cubed"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_abs"
        app:layout_constraintBottom_toBottomOf="@id/button_abs"
        app:layout_constraintLeft_toRightOf="@id/button_conj"
        app:layout_constraintRight_toLeftOf="@id/button_inverse"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_cubed"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_inverse"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_abs"
        app:layout_constraintBottom_toBottomOf="@id/button_abs"
        app:layout_constraintLeft_toRightOf="@id/button_cubed"
        app:layout_constraintRight_toRightOf="parent"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_inverse"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_npr"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toBottomOf="@id/button_abs"
        app:layout_constraintBottom_toTopOf="@id/button_delete"
        app:layout_constraintLeft_toLeftOf="@id/button_abs"
        app:layout_constraintRight_toRightOf="@id/button_abs"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_npr"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_ncr"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_npr"
        app:layout_constraintBottom_toBottomOf="@id/button_npr"
        app:layout_constraintLeft_toLeftOf="@id/button_arg"
        app:layout_constraintRight_toRightOf="@id/button_arg"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_ncr"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_log2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_npr"
        app:layout_constraintBottom_toBottomOf="@id/button_npr"
        app:layout_constraintLeft_toLeftOf="@id/button_conj"
        app:layout_constraintRight_toRightOf="@id/button_conj"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_log2"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_xroot"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_npr"
        app:layout_constraintBottom_toBottomOf="@id/button_npr"
        app:layout_constraintLeft_toLeftOf="@id/button_cubed"
        app:layout_constraintRight_toRightOf="@id/button_cubed"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_nroot"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_factorial"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetBottom="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_npr"
        app:layout_constraintBottom_toBottomOf="@id/button_npr"
        app:layout_constraintLeft_toLeftOf="@id/button_inverse"
        app:layout_constraintRight_toRightOf="@id/button_inverse"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_factorial"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_delete"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        app:layout_constraintTop_toBottomOf="@id/button_npr"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/button_clear"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_delete"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_clear"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_delete"
        app:layout_constraintBottom_toBottomOf="@id/button_delete"
        app:layout_constraintLeft_toRightOf="@id/button_delete"
        app:layout_constraintRight_toLeftOf="@id/button_more"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_clear"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_more"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:insetRight="@dimen/button_margin"
        app:layout_constraintTop_toTopOf="@id/button_delete"
        app:layout_constraintBottom_toBottomOf="@id/button_delete"
        app:layout_constraintLeft_toRightOf="@id/button_clear"
        app:layout_constraintRight_toLeftOf="@id/button_execute"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_more"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button_execute"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/button_delete"
        app:layout_constraintBottom_toBottomOf="@id/button_delete"
        app:layout_constraintLeft_toRightOf="@id/button_more"
        app:layout_constraintRight_toRightOf="parent"
        style="@style/AppWidget.Button.Calculator"
        android:text="@string/button_execute"/>

</androidx.constraintlayout.widget.ConstraintLayout>